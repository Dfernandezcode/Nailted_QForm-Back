    "start": "ts-node-dev --inspect -- ./src/index.ts",
    "seed:all": "ts-node-dev ./src/domain/utils/seed.ts",
    "start:pro": "npm run build && node ./dist/index.js",
    "build:swagger-json": "ts-node-dev ./src/utils/generate-swagger-json.ts --ingore-watch",
    "test:verbose": "cross-env DB_NAME=NAILTED-FORM-TEST jest --detectOpenHandles",
    "test": "cross-env DB_NAME=NAILTED-FORM-TEST jest --detectOpenHandles",
    "prepare": "husky install"
