  "scripts": {
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/templates/*.html src/templates/img/*.png dist/",
    "build": "npm run clean && tsc && npm run copy-files",
    "precommit": "npm run lint && npm run build",
    "lint": "eslint .",
